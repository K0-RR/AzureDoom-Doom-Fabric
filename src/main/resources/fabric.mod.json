{
	"schemaVersion": 1,
	"id": "doom",
	"version": "${version}",
	"name": "Doom Fabric",
	"description": "This is my Fabric port of my Forge Doom mod!",
	"authors": [
		"AzureDoomC"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/azuredooms-doom-mod-fabric/",
		"sources": "https://github.com/AzureDoom/AzureDoom-Doom-Fabric/"
	},
	"license": "MIT",
	"icon": "assets/doom/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"mod.azure.doom.DoomMod"
		],
		"client": [
			"mod.azure.doom.client.ClientInit"
		],
		"modmenu": [
			"mod.azure.doom.config.ModMenuDoom"
		]
	},
	"mixins": [
		"doom.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.11.3",
		"fabric": "*",
		"minecraft": "1.17.x",
		"trinkets": ">=3.0.1",
		"geckolib3": ">=3.0.9"
	},
	"suggests": {
		"flamingo": "*"
	},
	"accessWidener": "doom.accesswidener"
}
